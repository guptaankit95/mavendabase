<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>com.login.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ActionServlet</servlet-name>
		<display-name>ActionServlet</display-name>
		<description></description>
		<servlet-class>com.login.ActionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddUser</servlet-name>
		<display-name>AddUser</display-name>
		<description></description>
		<servlet-class>com.login.AddUser</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewUsers</servlet-name>
		<display-name>ViewUsers</display-name>
		<description></description>
		<servlet-class>com.login.ViewUsers</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserActivity</servlet-name>
		<display-name>UserActivity</display-name>
		<description></description>
		<servlet-class>com.login.UserActivity</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActionServlet</servlet-name>
		<url-pattern>/ActionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/AddUser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewUsers</servlet-name>
		<url-pattern>/ViewUsers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserActivity</servlet-name>
		<url-pattern>/UserActivity</url-pattern>
	</servlet-mapping>
</web-app>
